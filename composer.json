{
    "name": "softspring/doctrine-templates",
    "description": "This library provides doctrine templates",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "symfony/form": "^5.4|^6.0",
        "symfony/intl": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrineTemplates\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev"
        }
    }
}
