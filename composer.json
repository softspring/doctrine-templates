{
    "name": "softspring/doctrine-templates",
    "description": "This library provides doctrine templates",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/intl": "^5.4|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrineTemplates\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
